# Docker volume driver
export VOLUME_DRIVER=rbd

# Etcd Endpoints IP
export ETCD_CLIENT_IP=

# For docker swarm cluster
export DOCKER_SWARM_URI=
export COMPOSE_OPT=

# Versions
export MONGO_VERSION=3.4
export HAPROXY_VERSION=1.7

# Mongodb Information
export MONGO_INITDB_ROOT_USERNAME=siteRootAdmin
export MONGO_INITDB_ROOT_PASSWORD=password
export REPLSET_NAME=rs0

# Haproxy Information,The first mongo container(rs0node1) is primary,definition in initiate.sh.
export HAPROXY_NODE=
export MONGO_PRIMARY=rs0node1

